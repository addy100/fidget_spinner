   "source": [
    "from turtle import *"
   ]
  },
  {
   "cell_type": "raw",
   "metadata": {},
   "source": [
    "rev = {'turn': 0}\n",
    "def spinner();\n",
    "    clear()\n",
    "    angle = rev['turn']/10\n",
    "    right(angle)\n",
    "    forward(100)\n",
    "    dot(120, 'red')\n",
    "    back(100)\n",
    "    rirght(120)\n",
    "    forward(100)\n",
    "    \n",
    "    dot(120,'blue')\n",
    "    back(100)\n",
    "    right(120)\n",
    "    forward(100)\n",
    "    \n",
    "    dot(120,'green')\n",
    "    back(100)\n",
    "    right(120)\n",
    "    update()\n",
    "    \n",
    "def animate()\n",
    "    if rev['turn']>0:\n",
    "        rev['turn']-=1\n",
    "        \n",
    "    spinner()\n",
    "    ontimer(animate, 20)\n",
    "   \n",
    "def flick()\n",
    "    rev['turn']+=10\n",
    "    \n",
    "setup(420,420,370,0)\n",
    "hideturtle()\n",
    "tracer(False)\n",
    "width(20)\n",
    "onkey(flick, 'space')\n",
    "listen()\n",
    "animate()\n",
    "done()"
   ]
  },
